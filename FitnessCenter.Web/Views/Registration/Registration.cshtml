@model Registration

@{
    ViewData["Title"] = "Registration";
}



<div class="panel panel-default col-md-8 col-md-offset-2 top-buffer">
    <div class="panel-heading">
        <h2>You are registering for  scheduled on, please hit confirm button to register for it</h2>
    </div>
    <div class="panel-body col-md-4 top-buffer">
        @using (Html.BeginForm())
    {
        <div>@Html.HiddenFor(x=>x.Schedule.ID)</div>
        <button type="submit" class="btn btn-default">Confirm</button>
}
    </div>
</div>

